<mvc:View
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form"
	xmlns:m="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns="sap.m"
	xmlns:t="sap.ui.table"
	controllerName="ZHCM_YD_SKALA.controller.View1"
	height="100%"
>
	<Page
		title="{i18n>title}"
		showHeader="false"
		enableScrolling="true"
		class="sapUiNoContentPadding"
	>
		<!-- <Panel class="sapMShellGlobalOuterBackground"> -->
		<IconTabBar
			id="idIconTabBarStretchContent"
			select="handleIconTabBarSelect"
			backgroundDesign="Solid"
			applyContentPadding="false"
			expanded="true"
			class="sapUiResponsiveContentPadding"
		>
			<items>
				<IconTabFilter
					id="idfilter01"
					text="{i18n>Sub01}"
				>
					<content>
						<VBox>
							<f:Form
								id="FormDisplayDetail01"
								editable="true"
							>
								<f:layout>
									<f:ResponsiveGridLayout
										labelSpanL="3"
										labelSpanM="3"
										emptySpanL="4"
										emptySpanM="4"
										columnsL="1"
										columnsM="1"
									/>
								</f:layout>
								<f:formContainers>
									<f:FormContainer>
										<f:formElements>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>Werks}"
														labelFor="idulke"
														class="paddingLabel"
													/>
													<ComboBox
														id="idulke"
														change="onChangeUlke"
														class="sapUiSmallMarginBottom"
														selectedKey="{scaleView>/Country1}"
														items="{ path: '/ULKESet', 
															sorter: {
																path: 'Werks'
															}, 
															events:{
																dataRequested: '.onCountryRequested',
																dataReceived: '.onCountryReceived'
															}
														}"
														width="270px"
														busyIndicatorDelay="0"
													>
														<core:Item
															id="ulke"
															key="{Werks}"
															text="{Name1}"
														/>
													</ComboBox>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>Waers}"
														labelFor="idwaers"
														class="paddingLabel"
													/>
													<Input
														id="idwaers"
														editable="false"
														width="270px"
														class="sapUiSmallMarginBottom"
													/>
													<Label
														text="{i18n>Waersusd}"
														labelFor="idwaersusd"
														class="paddingLabel"
													/>
													<Input
														id="idwaersusd"
														editable="false"
														width="75%"
														class="sapUiSmallMarginBottom"
													/>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>Orgeh}"
														labelFor="idorgeh"
														class="paddingLabel"
													/>
													<ComboBox
														id="idorgeh"
														change="onChangeOrgeh"
														selectedKey="{scaleView>/OrgUnit1}"
														class="sapUiSmallMarginBottom"
														items="{
															path: 'OrgehModel>/OrgehList'
														}"
														width="270px"
													>
														<core:Item
															id="orgeh"
															key="{OrgehModel>Orgeh}"
															text="{OrgehModel>OrgehTx}"
														/>
													</ComboBox>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>Stell}"
														labelFor="idstell"
														class="paddingLabel"
													/>
													<ComboBox
														id="idstell"
														change="onChangeStell"
														class="sapUiSmallMarginBottom"
														items="{
															path: 'StellModel>/StellList'
														}"
														width="270px"
													>
														<core:Item
															id="stell"
															key="{StellModel>Stell}"
															text="{StellModel>StellTx}"
														/>
													</ComboBox>
													<Label
														text="{i18n>Betrgstell}"
														labelFor="idbetrg1"
														class="paddingLabel"
													/>
													<Input
														xmlns="sap.m"
														id="idbetrg1"
														editable="false"
														class="sapUiSmallMarginBottom"
														width="75%"
													/>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>Kidem}"
														labelFor="idkidem"
														class="paddingLabel"
													/>
													<Input
														id="idkidem"
														type="Number"
														liveChange="onLiveKidem"
														width="270px"
													/>
													<Label
														text="{i18n>Betrgkidem}"
														labelFor="idbetrg3"
														class="paddingLabel"
													/>
													<Input
														xmlns="sap.m"
														id="idbetrg3"
														editable="false"
														class="sapUiSmallMarginBottom"
														width="75%"
													/>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>Kidem2}"
														labelFor="idkidem2"
														class="paddingLabel"
													/>
													<Input
														id="idkidem2"
														type="Number"
														liveChange="onLiveKidem"
														class="sapUiSmallMarginBottom"
														width="270px"
													/>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>Kidem3}"
														labelFor="idkidem3"
														class="paddingLabel"
													/>
													<Input
														xmlns="sap.m"
														id="idkidem3"
														editable="false"
														class="sapUiSmallMarginBottom"
														width="270px"
													/>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>Slart}"
														labelFor="idslart"
														class="paddingLabel"
													/>
													<ComboBox
														id="idslart"
														change="onChangeSlart"
														selectedKey="{Slart}"
														items="{
															path: '/SLARTSet',
															sorter: {
																path: 'Slart'
															}
														}"
														width="270px"
													>
														<core:Item
															id="slart"
															key="{Slart}"
															text="{Stext}"
														/>
													</ComboBox>
													<Label
														text="{i18n>Betrgslart}"
														labelFor="idbetrg2"
														class="paddingLabel"
													/>
													<Input
														xmlns="sap.m"
														id="idbetrg2"
														editable="false"
														class="sapUiSmallMarginBottom"
														width="75%"
													/>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>TotalBetrg}"
														labelFor="idtotal"
														class="paddingLabel"
													/>
													<Input
														id="idtotal"
														type="Number"
														editable="false"
														width="270px"
														class="sapUiSmallMarginBottom"
													/>
													<Label
														text="{i18n>TotalBetrgUsd}"
														labelFor="idtotalusd"
														class="paddingLabel"
													/>
													<Input
														id="idtotalusd"
														type="Number"
														editable="false"
														width="75%"
														class="sapUiSmallMarginBottom"
													/>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>TotalBetrgTalep}"
														labelFor="idtotaltalep"
														class="paddingLabel"
													/>
													<Input
														id="idtotaltalep"
														type="Number"
														liveChange="onLiveTalep"
														width="270px"
														class="sapUiSmallMarginBottom"
													/>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text=""
														labelFor="idcalcbutton"
														class="paddingLabel"
													/>
													<Button
														text="{i18n>Calculate}"
														id="idcalcbutton"
														press="onPressCalculate"
														class="paddingButton"
													/>
												</l:HorizontalLayout>
											</f:FormElement>
										</f:formElements>
									</f:FormContainer>
								</f:formContainers>
							</f:Form>
							<FlexBox>
								<Label />
							</FlexBox>
							<items />
							<Panel
								expandable="true"
								expanded="true"
								headerText="{i18n>Explanation}"
								width="auto"
								class="sapUiResponsiveMargin"
							>
								<content>
									<FormattedText id="htmlTextOrg" />
								</content>
							</Panel>
						</VBox>
					</content>
				</IconTabFilter>
				<IconTabFilter
					id="idfilter02"
					text="{i18n>Sub02}"
				>
					<content>
						<VBox>
							<f:Form
								id="FormDisplayDetail02"
								editable="true"
							>
								<f:layout>
									<f:ResponsiveGridLayout
										labelSpanL="3"
										labelSpanM="3"
										emptySpanL="4"
										emptySpanM="4"
										columnsL="1"
										columnsM="1"
									/>
								</f:layout>
								<f:formContainers>
									<f:FormContainer>
										<f:formElements>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>Werks}"
														labelFor="idulke02"
														class="paddingLabel"
													/>
													<ComboBox
														id="idulke02"
														change="onChangeUlke02"
														class="sapUiSmallMarginBottom"
														selectedKey="{scaleView>/Country2}"
														items="{
															path: '/ULKESet',
															sorter: {
																path: 'Werks'
															}
														}"
														width="270px"
													>
														<core:Item
															id="ulke02"
															key="{Werks}"
															text="{Name1}"
														/>
													</ComboBox>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>Waers}"
														labelFor="idwaers02"
														class="paddingLabel"
													/>
													<Input
														id="idwaers02"
														editable="false"
														width="270px"
														class="sapUiSmallMarginBottom"
													/>
													<Label
														text="{i18n>Waersusd}"
														labelFor="idwaersusd02"
														class="paddingLabel"
													/>
													<Input
														id="idwaersusd02"
														editable="false"
														width="75%"
														class="sapUiSmallMarginBottom"
													/>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>Orgeh}"
														labelFor="idorgeh02"
														class="paddingLabel"
													/>
													<ComboBox
														id="idorgeh02"
														change="onChangeOrgeh02"
														class="sapUiSmallMarginBottom"
														selectedKey="{scaleView>/OrgUnit2}"
														items="{
															path: 'OrgehModel02>/OrgehList02'
														}"
														width="270px"
													>
														<core:Item
															id="orgeh02"
															key="{OrgehModel02>Orgeh}"
															text="{OrgehModel02>OrgehTx}"
														/>
													</ComboBox>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>Stell}"
														labelFor="idstell02"
														class="paddingLabel"
													/>
													<ComboBox
														id="idstell02"
														change="onChangeStell02"
														class="sapUiSmallMarginBottom"
														items="{
															path: 'StellModel02>/StellList02'
														}"
														width="270px"
													>
														<core:Item
															id="stell02"
															key="{StellModel02>Stell}"
															text="{StellModel02>StellTx}"
														/>
													</ComboBox>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<FlexBox>
													<Label />
												</FlexBox>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>Betrgstell}"
														labelFor="idbetrg1_02"
														class="paddingLabel"
													/>
													<Input
														xmlns="sap.m"
														id="idbetrg1_02"
														width="270px"
														editable="false"
														class="sapUiSmallMarginBottom"
													/>
													<Label
														text="{i18n>Betrgstellusd}"
														labelFor="idbetrg1usd_02"
														class="paddingLabel"
													/>
													<Input
														xmlns="sap.m"
														id="idbetrg1usd_02"
														editable="false"
														class="sapUiSmallMarginBottom"
														width="75%"
													/>
												</l:HorizontalLayout>
											</f:FormElement>
											<f:FormElement>
												<l:HorizontalLayout>
													<Label
														text="{i18n>Ztype}"
														labelFor="idztype1_02"
														class="paddingLabel"
													/>
													<Input
														xmlns="sap.m"
														id="idztype1_02"
														width="270px"
														editable="false"
														class="sapUiSmallMarginBottom"
													/>
												</l:HorizontalLayout>
											</f:FormElement>
										</f:formElements>
									</f:FormContainer>
								</f:formContainers>
							</f:Form>
							<FlexBox>
								<Label />
							</FlexBox>
							<items />
							<!--<Panel expandable="true" expanded="true" headerText="{i18n>Explanation}" width="auto" class="sapUiResponsiveMargin">-->
							<!--	<content>-->
							<!--		<FormattedText id="htmlTextOrg02"/>-->
							<!--	</content>-->
							<!--</Panel>-->
						</VBox>
					</content>
				</IconTabFilter>
			</items>
		</IconTabBar>
		<!-- </Panel> -->
	</Page>
</mvc:View>